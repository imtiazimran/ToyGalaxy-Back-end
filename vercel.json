{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "vercel"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "DELETE",
          "OPTION"
        ]
      }
    ]
  }